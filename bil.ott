defns reduce_exp :: '' ::=

 defn delta |- exp ~> exp' :: :: exp :: '' by

 (var,v) isin delta
------------------ :: var_in
 delta |- var ~> v


 id:type notin dom(delta)
 -------------------------------------------- :: var_unknown
